{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2015.collection",
      "es2015.iterable"
    ],
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "rootDir": "./src",
    "outDir": "./build",
    "importHelpers": true,
    "noEmitHelpers": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules/**"]
}
