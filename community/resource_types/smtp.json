{
  "workspace_id": "starter",
  "name": "smtp",
  "schema": {
    "type": "object",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "required": [
      "host"
    ],
    "properties": {
      "host": {
        "type": "string",
        "default": "mail.smtpbucket.com",
        "description": "SMTP host address"
      },
      "port": {
        "type": "integer",
        "default": 8025,
        "description": "port number on which to connect"
      },
      "user": {
        "type": "string",
        "description": ""
      },
      "password": {
        "type": "string",
        "description": ""
      }
    }
  },
  "description": ""
}
