		"autoprefixer": "^10.4.13",
		"cssnano": "^5.1.14",
		"d3-dag": "^0.11.5",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"monaco-editor-workers": "0.36.0",
		"ol": "^7.2.2",
		"openapi-typescript-codegen": "^0.23.0",
